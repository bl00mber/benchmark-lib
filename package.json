  "repository": {
    "type": "git",
    "url": "git+https://github.com/bl00mber/benchmark-lib.git"
  },
  "keywords": [
    "benchmark",
    "performance",
    "speed",
    "test",
    "testing",
    "react",
    "javascript"
  ],
  "files": [
    "LICENSE",
    "README.md",
    "dist/lib.js",
    "dist/style.css"
  ],
  "author": "Nick Reiley <bloomber111@gmail.com> (https://github.com/bl00mber)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bl00mber/benchmark-lib/issues"
  },
  "homepage": "https://github.com/bl00mber/benchmark-lib",
  "jest": {
    "moduleNameMapper": {
      ".(jpg|jpeg|png|gif|eot|otf|svg|ttf|woff|woff2|mp3)$": "<rootDir>/test/__mocks__/fileMock.js",
      ".(css|less|stylus|scss)$": "<rootDir>/test/__mocks__/styleMock.js"
    },
    "globals": {
      "__DEV__": false
    }
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "babel-loader": "^8.0.5",
    "babel-preset-react-app": "^7.0.2",
