{
  "name": "benchmark-lib",
  "version": "0.1.0",
  "description": "Tools for measuring functions relative performance",
  "main": "dist/lib.js",
  "scripts": {
    "test": "jest",
    "start": "TARGET=dev NODE_ENV=development webpack-dev-server --progress",
    "build": "TARGET=build NODE_ENV=production webpack -p --progress"
